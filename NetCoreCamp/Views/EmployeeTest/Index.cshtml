@using NetCoreCamp.Controllers
@model List<Class1>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Employee Name</th>
            <th>Sil</th>
            <th>Düzenle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EmployeeId</td>
                <td>@item.EmployeeName</td>
                <td><a href="/EmployeeTest/DeleteEmployee/@item.EmployeeId" class="btn btn-outline-danger">Sil</a></td>
                <td><a href="/EmployeeTest/EditEmployee/@item.EmployeeId" class="btn btn-outline-primary">Düzenle</a></td>
            </tr>

        }
    </tbody>
</table>
<a href="/EmployeeTest/AddEmployee/" class="btn btn-success">Yeni Çalışan Girişi</a>
